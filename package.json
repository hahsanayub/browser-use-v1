{
  "name": "browser-use",
  "version": "0.0.1",
  "description": "A TypeScript-first library for programmatic browser control, designed for building AI-powered web agents.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "commit": "yarn git-cz",
    "postinstall": "playwright install chromium",
    "postinstall:ci": "playwright install --with-deps chromium"
  },
  "keywords": [
    "browser",
    "use",
    "ai",
    "agent",
    "web",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unadlib/browser-use.git"
  },
  "author": "unadlib",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@aws-sdk/client-bedrock-runtime": "^3.936.0",
    "@cfworker/json-schema": "^4.1.1",
    "@google/genai": "^1.16.0",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "canvas": "^3.2.0",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "gif-encoder-2": "^1.0.5",
    "googleapis": "^166.0.0",
    "gpt-tokenizer": "^3.4.0",
    "groq-sdk": "^0.36.0",
    "ollama": "^0.6.3",
    "openai": "^5.13.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "playwright": "^1.54.2",
    "posthog-node": "^5.11.2",
    "turndown": "^7.2.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.2.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfkit": "^0.17.3",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
